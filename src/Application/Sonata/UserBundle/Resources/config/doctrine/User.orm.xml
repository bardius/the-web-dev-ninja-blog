<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\Sonata\UserBundle\Entity\User" table="fos_user_user">

        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        
        <field name="bakeFrequency" type="string" column="bakeFrequency" length="255" nullable="true" />
        <field name="sex" type="string" column="sex" length="255" nullable="true" />
        <field name="bakeChoises" type="array" column="bakeChoises" nullable="true" />
        <field name="age" type="string" column="age" length="255" nullable="true" />
        <field name="children" type="string" column="children" length="255" nullable="true" />
        <field name="campaign" type="string" column="campaign" length="255" nullable="true" />

    </entity>

</doctrine-mapping>
